{
    "name": "habibun/validation-bundle",
    "description": "Integration bundle for Respect\\Validation in Symfony",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Habibun Noby",
            "email": "habibun.noby@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2",
        "symfony/dependency-injection": "^4.0",
        "symfony/config": "^4.0",
        "symfony/http-kernel": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "Habibun\\ValidationBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Habibun\\ValidationBundle\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^5.3"
    }
}
